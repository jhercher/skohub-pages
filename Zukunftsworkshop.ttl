# Concept Scheme
zw:scheme a skos:ConceptScheme ;
    dc:title "Zukunftsworkshop Konzepte"@de ;
    dc:creator "Sven Gabor Janszky" ;
    dc:publisher "Johannes Hercher" ;
    dc:relation "https://coaching.janszky.de/wittmann/" ;
    dc:description "Thematisches Vokabular aus dem Zukunftsworkshop von 2b:ahead, automatisiert erstellt zu Testzwecken von Johannes Hercher"@de ;
    dcterms:created "2024-11-21"^^xsd:date ;
    skos:hasTopConcept zw:methodik,
                       zw:realitygap,
                       zw:gesundheit,
                       zw:ki,
                       zw:investment,
                       zw:mindset,
                       zw:personen,
                       zw:organisationen .

# =============================================================================
# TOP CONCEPT: METHODIK
# =============================================================================

zw:methodik a skos:Concept ;
    skos:prefLabel "Zukunftsmethodik"@de, "Future Methodology"@en ;
    skos:altLabel "Prognosemethodik"@de ;
    skos:definition "Wissenschaftliche Methoden zur Zukunftsplanung und Prognose"@de ;
    skos:inScheme zw:scheme ;
    skos:topConceptOf zw:scheme ;
    skos:narrower zw:backcasting,
                  zw:forecasting,
                  zw:tomorrowyourlife,
                  zw:zwölf-schritte .

zw:backcasting a skos:Concept ;
    skos:prefLabel "Backcasting"@de, "Backcasting"@en ;
    skos:definition "Planungsmethode, die von einem gewünschten Zukunftszustand ausgeht und rückwärts plant"@de ;
    skos:broader zw:methodik ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Im Gegensatz zum Forecasting wird vom Zielbild aus zurück zur Gegenwart geplant"@de ;
    skos:example "Chris Martin: 'In vier Jahren kennt uns die ganze Welt'"@de .

zw:forecasting a skos:Concept ;
    skos:prefLabel "Forecasting"@de, "Forecasting"@en ;
    skos:altLabel "Vorwärtsplanung"@de ;
    skos:definition "Traditionelle Planungsmethode von der Gegenwart in die Zukunft"@de ;
    skos:broader zw:methodik ;
    skos:inScheme zw:scheme ;
    skos:related zw:swot-analyse ;
    skos:scopeNote "Führt oft zur Optimierung des Status Quo statt zu transformativem Wachstum"@de .

zw:tomorrowyourlife a skos:Concept ;
    skos:prefLabel "Tomorrow Your Life Methode"@de ;
    skos:altLabel "TYL-Methode"@de ;
    skos:definition "3-Phasen-Methode zur Entwicklung des bestmöglichen Zukunfts-Ichs"@de ;
    skos:broader zw:methodik ;
    skos:inScheme zw:scheme ;
    skos:narrower zw:umfeld-erkennen,
                  zw:zukunfts-ich,
                  zw:weg-erreichen .

zw:zwölf-schritte a skos:Concept ;
    skos:prefLabel "12-Schritte-Methode"@de ;
    skos:definition "Detaillierte Schrittfolge innerhalb der Tomorrow Your Life Methode"@de ;
    skos:broader zw:tomorrowyourlife ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "4 Schritte pro Phase, insgesamt 12 Schritte"@de .

zw:umfeld-erkennen a skos:Concept ;
    skos:prefLabel "Umfeld erkennen"@de ;
    skos:definition "Erste Phase: Analyse wie das Umfeld in 5 Jahren aussieht"@de ;
    skos:broader zw:tomorrowyourlife ;
    skos:inScheme zw:scheme .

zw:zukunfts-ich a skos:Concept ;
    skos:prefLabel "Zukunfts-Ich entwickeln"@de ;
    skos:altLabel "Bestmögliches Zukunfts-Ich"@de ;
    skos:definition "Zweite Phase: Entwicklung des eigenen optimalen Zukunftsbildes"@de ;
    skos:broader zw:tomorrowyourlife ;
    skos:inScheme zw:scheme ;
    skos:related zw:zehn-zukunfts-ichs .

zw:weg-erreichen a skos:Concept ;
    skos:prefLabel "Weg erreichen"@de ;
    skos:definition "Dritte Phase: Planung und Umsetzung des Weges zum Zukunfts-Ich"@de ;
    skos:broader zw:tomorrowyourlife ;
    skos:inScheme zw:scheme ;
    skos:related zw:denkmuster-brechen .

zw:zehn-zukunfts-ichs a skos:Concept ;
    skos:prefLabel "10 Zukunfts-Ichs"@de ;
    skos:definition "Methode zur Identifikation von 10 möglichen besseren Zukunftsversionen"@de ;
    skos:broader zw:zukunfts-ich ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Alle 10 sind besser als der heutige Zustand, Ziel ist das Bestmögliche zu finden"@de ;
    skos:example "Svens Beispiel mit 28: Professor-Ich, Afrika-Ich, Weltreise-Ich, Sportler-Ich, Unternehmer-Ich, Silicon-Valley-Ich"@de .

zw:denkmuster-brechen a skos:Concept ;
    skos:prefLabel "Denkmuster brechen"@de ;
    skos:altLabel "Thomas-Tuchel-Methode"@de ;
    skos:definition "Methode zur Veränderung unterbewusster Verhaltensmuster"@de ;
    skos:broader zw:weg-erreichen ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "90-110 Tage für neues Denkmuster, altes Muster unmöglich machen"@de ;
    skos:note "Diamant-Trainingsplatz von Thomas Tuchel"@de .

zw:swot-analyse a skos:Concept ;
    skos:prefLabel "SWOT-Analyse"@de ;
    skos:altLabel "Stärken-Schwächen-Analyse"@de, "Plus-Minus-Liste"@de ;
    skos:definition "Traditionelle Analysemethode der Gegenwart"@de ;
    skos:broader zw:forecasting ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Führt zur Optimierung statt Transformation"@de .

# =============================================================================
# TOP CONCEPT: REALITY GAP
# =============================================================================

zw:realitygap a skos:Concept ;
    skos:prefLabel "Reality Gap"@de, "Reality Gap"@en ;
    skos:altLabel "Realitätslücke"@de ;
    skos:definition "Die Lücke zwischen zwei verschiedenen Wahrnehmungen der Zukunft"@de ;
    skos:inScheme zw:scheme ;
    skos:topConceptOf zw:scheme ;
    skos:narrower zw:blaue-linie,
                  zw:rote-linie .

zw:blaue-linie a skos:Concept ;
    skos:prefLabel "Blaue Linie"@de ;
    skos:altLabel "Loser-Linie"@de, "Pessimisten-Perspektive"@de ;
    skos:definition "Perspektive der langsamen inkrementellen Verbesserung, geprägt von Krisenbewusstsein"@de ;
    skos:broader zw:realitygap ;
    skos:inScheme zw:scheme ;
    skos:related zw:fixed-mindset ;
    skos:scopeNote "Typisch für deutsche/europäische öffentliche Meinung"@de ;
    skos:example "'Wenn wir viel Glück haben, wird es vielleicht ein bisschen besser'"@de .

zw:rote-linie a skos:Concept ;
    skos:prefLabel "Rote Linie"@de ;
    skos:altLabel "Wachstumslinie"@de, "Optimisten-Perspektive"@de ;
    skos:definition "Perspektive exponentiellen Wachstums und technologischer Lösungen"@de ;
    skos:broader zw:realitygap ;
    skos:inScheme zw:scheme ;
    skos:related zw:growth-mindset ;
    skos:scopeNote "Typisch für internationale Technologie-Elite"@de ;
    skos:example "'Wir lösen die größten Menschheitsprobleme in 20 Jahren'"@de .

# =============================================================================
# TOP CONCEPT: MINDSET
# =============================================================================

zw:mindset a skos:Concept ;
    skos:prefLabel "Mindset"@de, "Mindset"@en ;
    skos:altLabel "Denkweise"@de, "Geisteshaltung"@de ;
    skos:definition "Grundlegende Überzeugungen über Intelligenz und Entwicklungsfähigkeit"@de ;
    skos:inScheme zw:scheme ;
    skos:topConceptOf zw:scheme ;
    skos:narrower zw:fixed-mindset,
                  zw:growth-mindset .

zw:fixed-mindset a skos:Concept ;
    skos:prefLabel "Fixed Mindset"@de, "Fixed Mindset"@en ;
    skos:altLabel "Statisches Mindset"@de ;
    skos:definition "Überzeugung, dass Intelligenz angeboren und unveränderbar ist"@de ;
    skos:broader zw:mindset ;
    skos:inScheme zw:scheme ;
    skos:related zw:blaue-linie ;
    skos:scopeNote "Führt zu Vermeidung von Herausforderungen und Stagnation"@de ;
    dc:source "Carol Dweck, Stanford University" .

zw:growth-mindset a skos:Concept ;
    skos:prefLabel "Growth Mindset"@de, "Growth Mindset"@en ;
    skos:altLabel "Wachstums-Mindset"@de, "Zukunftsmindset"@de ;
    skos:definition "Überzeugung, dass 50% der Intelligenz während des Lebens entwickelbar ist"@de ;
    skos:broader zw:mindset ;
    skos:inScheme zw:scheme ;
    skos:related zw:rote-linie ;
    skos:scopeNote "Führt zu Anstrengung, Lernen aus Fehlern und kontinuierlichem Wachstum"@de ;
    dc:source "Carol Dweck, Stanford University" .

zw:umfeld-gestalten a skos:Concept ;
    skos:prefLabel "Umfeld gestalten"@de ;
    skos:altLabel "70%-Regel"@de ;
    skos:definition "Methode zur Entwicklung eines Growth Mindsets durch bewusste Umfeldgestaltung"@de ;
    skos:broader zw:growth-mindset ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Mehr als 50% (empfohlen 70%) täglicher Informationen von Growth Mindsets beziehen"@de .

# =============================================================================
# TOP CONCEPT: GESUNDHEIT & LANGLEBIGKEIT
# =============================================================================

zw:gesundheit a skos:Concept ;
    skos:prefLabel "Gesundheit und Langlebigkeit"@de, "Health and Longevity"@en ;
    skos:definition "Technologische Entwicklungen zur Verlängerung und Verbesserung des menschlichen Lebens"@de ;
    skos:inScheme zw:scheme ;
    skos:topConceptOf zw:scheme ;
    skos:narrower zw:genomanalyse,
                  zw:mikrobiom,
                  zw:gentherapie,
                  zw:precision-medicine,
                  zw:organtechnologie,
                  zw:3d-food-printing .

zw:genomanalyse a skos:Concept ;
    skos:prefLabel "Genomanalyse"@de, "Genome Analysis"@en ;
    skos:altLabel "Genetische Analyse"@de ;
    skos:definition "Analyse des individuellen Genoms zur Vorhersage genetisch bedingter Krankheiten"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:related zw:bgi ;
    skos:scopeNote "Kosten gesunken von Milliarden auf ca. 100 USD, Dateigröße 6GB"@de ;
    skos:example "8B 6GB 99 Formel: 8 Milliarden Menschen, 6 Gigabyte Daten, 99 Jahre Lebenserwartung"@de .

zw:mikrobiom a skos:Concept ;
    skos:prefLabel "Mikrobiom"@de, "Microbiome"@en ;
    skos:altLabel "Darmbakterien"@de, "Bakterien-Mix"@de ;
    skos:definition "Gesamtheit der Mikroorganismen im Darm, entscheidend für Gesundheit"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:related zw:intelligente-wcs, zw:3d-food-printing ;
    skos:scopeNote "Individuelle Optimierung verhindert Ausbruch genetisch angelegter Krankheiten"@de .

zw:intelligente-wcs a skos:Concept ;
    skos:prefLabel "Intelligente WCs"@de, "Smart Toilets"@en ;
    skos:definition "Toiletten mit QR-Code-Scan zur Analyse des Mikrobiom-Ist-Zustands"@de ;
    skos:broader zw:mikrobiom ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Bereits verfügbar in China, Shenzhen"@de .

zw:gentherapie a skos:Concept ;
    skos:prefLabel "Gentherapie"@de, "Gene Therapy"@en ;
    skos:definition "Therapeutische Veränderung von Genen zur Heilung oder Prävention von Krankheiten"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:related zw:telomere ;
    skos:scopeNote "Kosten 750.000 USD Volldosis, 375.000 USD halbe Dosis, hält 10-12 Jahre"@de ;
    skos:example "Liz Parrish: Biologisches Alter von 60 auf 22 Jahre reduziert"@de .

zw:telomere a skos:Concept ;
    skos:prefLabel "Telomere"@de, "Telomeres"@en ;
    skos:definition "Endstücke der Chromosomen, deren Länge das biologische Alter anzeigt"@de ;
    skos:broader zw:gentherapie ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Verlängung durch Gentherapie möglich"@de .

zw:precision-medicine a skos:Concept ;
    skos:prefLabel "Precision Medicine"@de, "Precision Medicine"@en ;
    skos:altLabel "Präzisionsmedizin"@de, "Personalisierte Medizin"@de ;
    skos:definition "Medizinischer Ansatz basierend auf individuellen genetischen und biometrischen Daten"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:related zw:full-body-scan, zw:early-detection ;
    skos:scopeNote "8.000 USD Jahresabo beim Human Longevity Institute"@de .

zw:full-body-scan a skos:Concept ;
    skos:prefLabel "Full Body Scan"@de, "Full Body Scan"@en ;
    skos:altLabel "Ganzkörper-Computertomographie"@de ;
    skos:definition "90-minütige CT-Untersuchung des gesamten Körpers"@de ;
    skos:broader zw:precision-medicine ;
    skos:inScheme zw:scheme .

zw:early-detection a skos:Concept ;
    skos:prefLabel "Early Detection"@de, "Early Detection"@en ;
    skos:altLabel "Früherkennung"@de ;
    skos:definition "Erkennung von Krankheiten bis zu 5 Jahre vor Ausbruch"@de ;
    skos:broader zw:precision-medicine ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Die meisten Krebsarten sind bei Früherkennung heilbar"@de .

zw:organtechnologie a skos:Concept ;
    skos:prefLabel "Organtechnologie"@de, "Organ Technology"@en ;
    skos:altLabel "Tissue Engineering"@de ;
    skos:definition "Technologie zur Herstellung und Konservierung von Ersatzorganen"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:related zw:kryokonservierung ;
    skos:scopeNote "Investment-Case: 25x Rendite in 5 Jahren"@de .

zw:kryokonservierung a skos:Concept ;
    skos:prefLabel "Kryokonservierung"@de, "Cryopreservation"@en ;
    skos:definition "Einfrieren und Auftauen von menschlichem Gewebe ohne Kristallbildung"@de ;
    skos:broader zw:organtechnologie ;
    skos:example zw:xiaoxi-wei ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Entwickelt an Berkeley University"@de .

zw:3d-food-printing a skos:Concept ;
    skos:prefLabel "3D-Food-Printing"@de, "3D Food Printing"@en ;
    skos:altLabel "Personalisierte Nahrung"@de ;
    skos:definition "3D-Druck von Nahrung angepasst an individuelles Mikrobiom"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:related zw:mikrobiom ;
    skos:scopeNote "Druckt Burger-Patties, Hash Browns, Cookies in 3 Minuten"@de ;
    skos:example "Rachel Weissman, Tel Aviv, Börsengang nach 12 Monaten"@de .

zw:lebenserwartung a skos:Concept ;
    skos:prefLabel "Lebenserwartung"@de, "Life Expectancy"@en ;
    skos:definition "Prognose der menschlichen Lebensspanne durch technologische Entwicklungen"@de ;
    skos:broader zw:gesundheit ;
    skos:inScheme zw:scheme ;
    skos:narrower zw:lebenserwartung-80-100,
                  zw:lebenserwartung-100-150 .

zw:lebenserwartung-80-100 a skos:Concept ;
    skos:prefLabel "Lebenserwartung 80-100 Jahre"@de ;
    skos:definition "Steigerung durch bestehende Technologien in den nächsten 20 Jahren"@de ;
    skos:broader zw:lebenserwartung ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Durch Precision Medicine, Early Detection, AI"@de .

zw:lebenserwartung-100-150 a skos:Concept ;
    skos:prefLabel "Lebenserwartung 100-150 Jahre"@de ;
    skos:definition "Weitere Steigerung durch Stammzelltherapien, Gentherapien, Tissue Engineering"@de ;
    skos:broader zw:lebenserwartung ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Prognose: 25-30 Jahre (schneller als die 100-200 Jahre die manche annehmen)"@de .

# =============================================================================
# TOP CONCEPT: KÜNSTLICHE INTELLIGENZ
# =============================================================================

zw:ki a skos:Concept ;
    skos:prefLabel "Künstliche Intelligenz"@de, "Artificial Intelligence"@en ;
    skos:altLabel "KI"@de, "AI"@en ;
    skos:definition "Technologien und Systeme mit maschineller Intelligenz"@de ;
    skos:inScheme zw:scheme ;
    skos:topConceptOf zw:scheme ;
    skos:narrower zw:ki-tools,
                  zw:ai-agents,
                  zw:humanoide-roboter,
                  zw:autonome-fahrzeuge,
                  zw:ki-molekularbiologie .

zw:ki-tools a skos:Concept ;
    skos:prefLabel "KI-Tools"@de, "AI Tools"@en ;
    skos:altLabel "ChatGPT"@de ;
    skos:definition "Einzelne KI-Anwendungen und Werkzeuge"@de ;
    skos:broader zw:ki ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Übergangsphase, veraltet seit 2025"@de .

zw:ai-agents a skos:Concept ;
    skos:prefLabel "AI Agents"@de, "AI Agents"@en ;
    skos:altLabel "KI-Agenten"@de ;
    skos:definition "Spezialisierte KI-Systeme die in Teams zusammenarbeiten"@de ;
    skos:broader zw:ki ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Wechsel von Tools zu Agents wurde 2018 für 2025 prognostiziert und traf ein"@de ;
    skos:example "70.000+ Agents weltweit verfügbar"@de .

zw:agent-team a skos:Concept ;
    skos:prefLabel "Agent-Team"@de ;
    skos:altLabel "KI-Team"@de ;
    skos:definition "Zusammenarbeit mehrerer spezialisierter AI Agents"@de ;
    skos:broader zw:ai-agents ;
    skos:inScheme zw:scheme ;
    skos:narrower zw:problem-analyzer-agent,
                  zw:projektmanager-agent,
                  zw:inspektor-agent .

zw:problem-analyzer-agent a skos:Concept ;
    skos:prefLabel "Problem-Analyzer-Agent"@de ;
    skos:definition "Agent zur Zerlegung komplexer Probleme in Unter-Probleme"@de ;
    skos:broader zw:agent-team ;
    skos:inScheme zw:scheme .

zw:projektmanager-agent a skos:Concept ;
    skos:prefLabel "Projektmanager-Agent"@de ;
    skos:definition "Agent zur Koordination und Delegation an Spezial-Agents"@de ;
    skos:broader zw:agent-team ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Kennt alle 70.000+ verfügbaren Agents und deren Fähigkeiten"@de .

zw:inspektor-agent a skos:Concept ;
    skos:prefLabel "Inspektor-Agent"@de ;
    skos:altLabel "Qualitätskontroll-Agent"@de ;
    skos:definition "Agent zur Prüfung von Antworten und Einhaltung von Regeln"@de ;
    skos:broader zw:agent-team ;
    skos:inScheme zw:scheme .

zw:humanoide-roboter a skos:Concept ;
    skos:prefLabel "Humanoide Roboter"@de, "Humanoid Robots"@en ;
    skos:definition "Menschenähnliche Roboter mit KI-Steuerung"@de ;
    skos:broader zw:ki ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "80 Unternehmen weltweit, 60 in China, Start 2026, Preise 2.000-30.000 USD"@de ;
    skos:example "Tesla Optimus, mehr Roboter als Menschen prognostiziert"@de .

zw:autonome-fahrzeuge a skos:Concept ;
    skos:prefLabel "Autonome Fahrzeuge"@de, "Autonomous Vehicles"@en ;
    skos:altLabel "Robo-Taxis"@de, "Selbstfahrende Autos"@de ;
    skos:definition "Fahrzeuge mit KI-Steuerung ohne menschlichen Fahrer"@de ;
    skos:broader zw:ki ;
    skos:inScheme zw:scheme ;
    skos:related zw:adaptive-produkte ;
    skos:scopeNote "30 Städte weltweit, Luxemburg als erste in Europa"@de ;
    skos:example "Pony.ai, 1 Mrd. USD Funding, Börsengang Hongkong"@de .

zw:adaptive-produkte a skos:Concept ;
    skos:prefLabel "Adaptive Produkte"@de, "Adaptive Products"@en ;
    skos:definition "Produkte die sich automatisch an Nutzer und Situation anpassen"@de ;
    skos:broader zw:autonome-fahrzeuge ;
    skos:inScheme zw:scheme ;
    skos:example "Auto verwandelt sich in Bett, Büro, Restaurant, Spielplatz"@de .

zw:ki-molekularbiologie a skos:Concept ;
    skos:prefLabel "KI in der Molekularbiologie"@de ;
    skos:altLabel "Molecular AI"@en ;
    skos:definition "Anwendung von KI zur Entwicklung neuer Proteine, Enzyme und Moleküle"@de ;
    skos:broader zw:ki ;
    skos:inScheme zw:scheme ;
    skos:scopeNote "Prognose für die nächsten 5 Jahre"@de ;
    dc:source "Richard Socher" .

zw:ki-praxisbeispiel-fpz a skos:Concept ;
    skos:prefLabel "KI-Praxisbeispiel FPZ"@de ;
    skos:definition "Frank Schifferdecker, FPZ GmbH, 5x Neukundengewinnung in 7 Monaten"@de ;
    skos:broader zw:ai-agents ;
    skos:inScheme zw:scheme ;
    skos:example "25 Mitarbeiter, Gesundheitsbranche, Rückenschmerz-Produkte"@de .

# =============================================================================
# TOP CONCEPT: INVESTMENT
# =============================================================================

zw:investment a skos:Concept ;
    skos:prefLabel "Investment"@de, "Investment"@en ;
    skos:altLabel "Investition"@de, "Kapitalanlage"@de ;
    skos:definition "Finanzielle Beteiligungen an zukunftsorientierten Unternehmen"@de ;
    skos:inScheme zw:scheme ;
    skos:topConceptOf zw:scheme ;
    skos:narrower zw:deep-tech,
                  zw:ki-startups,
                  zw:gesundheits-tech,
                  zw:bitcoin,
                  zw:tokenisierung .

zw:deep-tech a skos:Concept ;
    skos:prefLabel "Deep Tech"@de, "Deep Tech"@en ;
    skos:definition "Technologieunternehmen mit wissenschaftlich fundierter Innovation"@de ;
    skos:broader zw:investment ;
    skos:inScheme zw:scheme ;
    skos:example "100x Rendite bei Quantencomputer-Startup"@de .

zw:ki-startups a skos:Concept ;
    skos:prefLabel "KI-Startups"@de, "AI Startups"@en ;
    skos:definition "Junge Unternehmen im Bereich Künstliche Intelligenz"@de ;
    skos:broader zw:investment ;
    skos:inScheme zw:scheme ;
    skos:example "Perplexity: 36x Rendite in Monaten, 18 Mrd. USD Bewertung"@de .

zw:gesundheits-tech a skos:Concept ;
    skos:prefLabel "Gesundheitstechnologie"@de, "Health Technology"@en ;
    skos:altLabel "HealthTech"@en, "MedTech"@en ;
    skos:definition "Technologische Innovationen im Gesundheitsbereich"@de ;
    skos:broader zw:investment ;
    skos:inScheme zw:scheme ;
    skos:example "Neurotechnologie: 12x Rendite in 6 Monaten; Organtechnologie: 25x in 5 Jahren"@de .

